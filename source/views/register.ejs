<%- include ('partials/head') -%>

    <form action="../routes/create" method="POST" enctype="multipart/form-data">
        <main class="main-register">
            <h2>Crear cuenta</h2>
            <div class="container">
                <section class="nombre">
                    <label for="nombre">Nombre</label>
                    <input type="text" name="nombre" />
                </section>
                <section class="email">
                    <label for="email">Email</label>
                    <input type="email" name="email" />
                </section>
                <section class="telefono">
                    <label for="telefono">Telefono</label>
                    <input type="number" name="telefono" />
                </section>
                <section class="password">
                    <label for="password">Contraseña</label>
                    <input type="password" name="password" />
                </section>
                <section class="password">
                    <label for="password">Confirmar contraseña</label>
                    <input type="password" name="confirmarPassword" />
                </section>
                <section class="images">
                    <label for="images">Ingresar imagen</label>
                    <input type="file" name="foto" />
                </section>
                <section class="botones">
                    <button type="submit" id="registrar">Registrarse</button>
                    <button type="button">Iniciar sesión</button>
                </section>
            </div>
        </main>


    </form>
    <%- include ('partials/footer') -%>